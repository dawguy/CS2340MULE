package gameObjects;

/**
 * The Map which holds the tile elements
 * @author DavidW
 *
 */
public class Map {
	Tile[][] tiles;
	
	public Map(){
		tiles = new Tile[9][5];
		for(int i = 0; i < tiles.length; i++){
			for(int i = 0; i < tiles[i].length; i++){
				tiles = new Tile();
			}
		}
	}
	
	public void loadTextures(){
		
	}
}
